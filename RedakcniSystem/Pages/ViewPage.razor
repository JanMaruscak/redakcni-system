@page "/page/{name}"
@using RedakcniSystem.Data
@inject PagesService PagesService

@if (Page != null)
{
    @((MarkupString)Page.Html)
}

@code{
    [Parameter]
    public string Name { get; set; }

    public Page Page;
    protected override void OnInitialized()
    {
        Page = PagesService.GetPageByName(Name);
    }

}